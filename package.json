{
  "name": "ship-fast-code",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sst_dev": "sst dev next dev",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next dev",
    "lint": "next lint",
    "deploy-dev": "export ENV=development && export AWS_DEFAULT_PROFILE=default && sst deploy",
    "deploy-prod": "export ENV=production && export AWS_DEFAULT_PROFILE=default && sst deploy --stage=production",
    "generate-dev": "export ENV=development && drizzle-kit generate:sqlite --out ./libs/drizzle/migrations/development --breakpoints --schema=./libs/drizzle/schema.ts",
    "generate-prod": "export ENV=production && drizzle-kit generate:sqlite --out ./libs/drizzle/migrations/production --breakpoints --schema=./libs/drizzle/schema.ts",
    "migrate-dev": "export ENV=development && tsx ./libs/drizzle/migrate.ts",
    "migrate-prod": "export ENV=production && tsx ./libs/drizzle/migrate.ts",
    "studio-dev": "export ENV=development && drizzle-kit studio --port 3333",
    "studio-prod": "export ENV=production && drizzle-kit studio --port 3333",
    "create-db-dev": "export ENV=development && turso auth && turso db create blogfast-dev",
    "create-db-prod": "export ENV=production && turso auth && turso db create blogfast-prod",
    "create-db-token-dev": "export ENV=development && turso auth && turso db tokens create blogfast-dev",
    "create-db-token-prod": "export ENV=production && turso auth && turso db tokens create blogfast-prod",
    "show-db-url-dev": "export ENV=development && turso auth && turso db show blogfast-dev --url",
    "show-db-url-prod": "export ENV=production && turso auth && turso db show blogfast-prod --url"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.0.1",
    "@auth/mongodb-adapter": "^1.0.0",
    "@aws-lite/client": "^0.21.7",
    "@aws-lite/dynamodb": "^0.3.4",
    "@gomomento/sdk-web": "^1.86.0",
    "@headlessui/react": "^1.7.17",
    "@libsql/client": "^0.6.0",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.4.19",
    "@octokit/webhooks": "^13.2.7",
    "aws-sdk": "^2.1623.0",
    "axios": "^1.4.0",
    "crisp-sdk-web": "^1.0.21",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.10",
    "drizzle-zod": "^0.5.1",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "exponential-backoff": "^3.1.1",
    "form-data": "^4.0.0",
    "libsodium-wrappers": "^0.7.13",
    "mailgun.js": "^9.2.1",
    "mongodb": "^5.8.0",
    "mongoose": "^7.4.4",
    "next": "^14.0.0",
    "next-auth": "^4.24.7",
    "next-plausible": "^3.11.3",
    "next-sitemap": "^4.2.2",
    "nextjs-toploader": "^1.4.2",
    "nodemailer": "^6.9.13",
    "open-next": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.21.1",
    "resend": "^3.2.0",
    "rxjs": "^7.8.1",
    "sodium-native": "^3.0.0",
    "sst": "^3.0.1",
    "stripe": "^13.2.0",
    "swr": "^2.2.5",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@aws-lite/dynamodb-types": "^0.3.6",
    "@types/jest": "^29.5.4",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/mdx": "^2.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.5.8",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/sodium-native": "^2.3.9",
    "autoprefixer": "^10.4.15",
    "daisyui": "^3.6.1",
    "drizzle-kit": "^0.20.18",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.9.3",
    "typescript": "^5.2.2"
  }
}
